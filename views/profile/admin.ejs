<h1>Admin Page: <%= user.displayname %></h1>

<div class="row">
	<div class="col s12 m4">
		<img src="<%=user.pic%>" alt="profile pic">
	</div>
	<div class="col s12 m4">
		<p><strong>Name:</strong> <%= user.firstname %> <%= user.lastname %></p>
		<p><strong>Birthday:</strong> <%= moment(user.birthday).format('dddd MMMM Do[,] YYYY') %></p>
		<p><strong>Zipcode:</strong> <%= user.zipcode %></p>
		<p><strong>Account Created On:</strong> <%= moment(user.createdAt).format('dddd MMMM Do[,] YYYY') %></p>
	</div>
	<div class="col s12 m4">
		<p><strong>User Profiles</strong></p>
		<% users.forEach(u => { %>
			<div>
				<a href="/profile/guest/<%=u.id%>">
					<%= u.displyname %>
					<% if (user.id == u.id) { %>
						(This is your profile!)
					<% } %>
				</a>
			</div>
		<% }) %>
	</div>
</div>