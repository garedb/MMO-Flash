<% if (alerts && alerts.error) { %>
		<div class="alert-card red lighten-2" style="margin-top: 0px">
			<div class="card-content" style="padding: 0px">
				<% alerts.error.forEach(e => { %>
					 <%= e %>
				<% }) %>
			</div>	
		</div>
<% } %>

<% if (alerts && alerts.success) { %>
		<div class="alert-card green lighten-2" style="margin-top: 0px">
			<div class="card-content" style="padding: 0px">
				<% alerts.success.forEach(s => { %>
					<p> <%= s %> </p>
				<% }) %>
			</div>	
		</div>
<% } %>